    ''' <summary>
    ''' event click row cua grid -> gan du lieu len cac control textbox tuong ung
    ''' rep.GetCommendLevelID(hfID.Value)-> lay du lieu cap khen thuong theo ma cap khen thuong
    ''' </summary>
    ''' <param name="sender"></param>
    ''' <param name="e"></param>
    ''' <remarks></remarks>
    Private Sub rgMain_SelectedIndexChanged(ByVal sender As Object, ByVal e As System.EventArgs) Handles rgMain.SelectedIndexChanged
        Dim method As String = System.Reflection.MethodBase.GetCurrentMethod().Name.ToString()
        Try
            Dim startTime As DateTime = DateTime.UtcNow
            Dim rep As New ProfileRepository
            If rgMain.SelectedItems.Count > 0 Then
                Dim slItem As GridDataItem
                slItem = rgMain.SelectedItems(0)
                hfID.Value = slItem("ID").Text
                Dim objCommendLevel = rep.GetCommendLevelID(hfID.Value)
                If objCommendLevel IsNot Nothing Then
                    txtCode.Text = objCommendLevel.CODE
                    txtNameVN.Text = objCommendLevel.NAME
                    txtRemark.Text = objCommendLevel.REMARK
                    If objCommendLevel.COMMEND_LEVEL IsNot Nothing Then
                        rntxtLevel.Value = objCommendLevel.COMMEND_LEVEL
                    End If
                    CommendLevel = objCommendLevel

                End If
            End If
            rep.Dispose()
            _myLog.WriteLog(_myLog._info, _classPath, method,
                                   CLng(DateTime.UtcNow.Subtract(startTime).TotalSeconds).ToString(), Nothing, "")
        Catch ex As Exception
            _myLog.WriteLog(_myLog._error, _classPath, method, 0, ex, "")

        End Try

    End Sub
